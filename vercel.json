{
  "version": 2,
  "builds": [
    { "src": "apps/web/index.html", "use": "@vercel/static-build", "config": { "distDir": "apps/web/dist" } },
    { "src": "api/serverless.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/serverless.ts" },
    { "src": "/(.*)", "dest": "apps/web/dist/$1" }
  ]
}
